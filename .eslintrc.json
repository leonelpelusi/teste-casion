{
	"env": {
		"es2020": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"rules": {
		"semi": 0,
		"comma-dangle": ["error", "never"],
		"indent": ["error", 2],
		"import/prefer-default-export": 0,
		"no-console": "off",
		"space-before-function-paren": "off",
		"no-param-reassign":"off"
	}
}
